# Environment variables for markdown-vault
# Copy this file to .env and update with your values

# API Key (will be auto-generated if not set)
MARKDOWN_VAULT_API_KEY=

# Vault path
MARKDOWN_VAULT_PATH=/path/to/your/vault

# Server configuration
MARKDOWN_VAULT_HOST=127.0.0.1
MARKDOWN_VAULT_PORT=27123

# Logging level (DEBUG, INFO, WARNING, ERROR)
MARKDOWN_VAULT_LOG_LEVEL=INFO
